{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@lib/*": [
        "./src/lib/*"
      ],
      "@constants": [
        "./src/constants.ts"
      ]
    }
  },
  "include": [
    "./src/**/*",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
